<div class  ="form-horizontal" role="form">
<%= form_for(@practice) do |f| %>

  <div class="hidden_field">
    <%= f.hidden_field :circle_id,  :value => @circle_id %>
  </div>

  <div class="form-group">
    <div class="field">
      <%= f.label :man_rane, "男子レーン数",:class=>"col-sm-6 control-label" %>
      <div class="col-sm-2">
        <%= f.number_field :man_rane,:class=>"form-control" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <div class="field">
      <%= f.label :mix_rane, "ミックスレーン数",:class=>"col-sm-6 control-label" %>
      <div class="col-sm-2">
        <%= f.number_field :mix_rane,:class=>"form-control" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <div class="field">
      <%= f.label :method, "練習",:class=>"col-sm-6 control-label" %>
      <div class="col-sm-2">
        <%= f.select :method,{"ストスト"=>"stroke","ボレスト"=>"volley"},{},
        {:class=>"form-control"}%>
      </div>
    </div>
  </div>

  <div class="form-group submit">
      <div class="center">
          <%= f.submit "Confirm", :class => "btn btn-default" %>
      </div>
  </div>

<% end %>
</div>
