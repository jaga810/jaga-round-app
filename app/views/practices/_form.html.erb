<div class  ="form-horizontal" role="form">
<%= form_for(@practice) do |f| %>
  <% if @practice.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@practice.errors.count, "error") %> prohibited this practice from being saved:</h2>

      <ul>
      <% @practice.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-group">
    <div class="field">
      <%= f.label :circle_id, "サークルid",:class=>"col-sm-6 control-label" %>
      <div class="col-sm-2">
        <%= f.number_field :circle_id,:class=>"form-control" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <div class="field">
      <%= f.label :man_rane, "男子レーン数",:class=>"col-sm-6 control-label" %>
      <div class="col-sm-2">
        <%= f.number_field :man_rane,:class=>"form-control" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <div class="field">
      <%= f.label :mix_rane, "ミックスレーン数",:class=>"col-sm-6 control-label" %>
      <div class="col-sm-2">
        <%= f.number_field :mix_rane,:class=>"form-control" %>
      </div>
    </div>
  </div>

  <div class="form-group submit">
      <div class="center">
          <%= f.submit "Confirm", :class => "btn btn-default" %>
      </div>
  </div>

<% end %>
</div>
